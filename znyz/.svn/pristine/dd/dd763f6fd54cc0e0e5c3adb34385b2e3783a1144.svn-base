<script>

var user1 =localStorage.getItem("user");
var user = JSON.parse(user1);
// $("#headerSet").val('')
$('#headerSet').on('click', function() {
    console.log(111111111111)
						var index = layer.open({
							type: 2,
							content: 'headerSet.html',
                            area: ['650px', '600px'],
                            title: ['个人信息'],
							maxmin: true
						});
						//					layer.full(index);
                    });


                layui.use(['form','laydate'], function(){
                var form = layui.form;
                var $ = layui.$;
                var laydate = layui.laydate;
                BindUpdateList()
                function BindUpdateList(){
                        $.ajax({
                        type: "GET",
                        url: commonIP + 'api/system/user/get?authToken='+authToken+'&page=1&limit=1&id='+user.id,
                        parseData: function(res){ //res 即为原始返回的数据
                        return {
                          "code": res.code, //解析接口状态
                          "msg": res.msg, //解析提示文本
                          "count": res.count, //解析数据长度
                          "data": res.dataSource.list //解析数据列表
                        };
                          },
                        contentType: 'application/json',
                        async: false,
                        success: function(data) {
                            var datas = JSON.parse(data);
                            if (datas.errcode == 200) {
                                //赋值
                                console.log(datas)
                                // $("#headerSetText").val(datas.dataSource.list[0].loginName);
                                var loginName= datas.dataSource.list[0].loginName
                                document.getElementById('headerSetText').innerHTML=loginName
                                form.render();
                            }
                        }
                        });
                        }
                    })
</script>
