		<!-- 内容 -->
		<div class="parent">
			<div class="login_left">
				<img src="statics/img/mobile_index.png" class="ml">
				<div class="login">
					<div class="login_cont">
						<h1>生猪智能养殖管理系统</h1>
						<form class="layui-form">
							<div class="layui-form-item">
								<input type="text" id="username" name="username" lay-verify="required" lay-verType="tips" autocomplete="off"
								 data-i18n="[placeholder]login.username" placeholder="请输入您的用户名" />
							</div>
							<div class="layui-form-item">
								<input type="password" id="password" name="password" value="" lay-verify="required" lay-verType="tips"
								 autocomplete="off" data-i18n="[placeholder]login.password" placeholder="请输入您的密码" />
							</div>
							<div class="layui-form-item">
								<select id="roleID" name="roleID" lay-verify="required|roleID" style="">
									<option value="" data-i18n="login.selectRole"></option>
								</select>
							</div>
							<a href="#">
								<div class="btn" lay-submit="" lay-filter="login" data-i18n="login.btn"></div>
							</a>
						</form>
					</div>
				</div>

			</div>
			<div class="child">
				<img src="statics/img/pig_img.jpg" width="100%" />
			</div>
		</div>
