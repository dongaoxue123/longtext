<script>
	var authToken = '';
	var pigsNumber=''
	var farmId = '';
	$(document).ready(function() {
		// 取得authToken
		authToken = localStorage.getItem('authToken');
		var user = localStorage.getItem('user');
		farmId = JSON.parse(user).farmId;
		if (authToken.length = 0) {
			alert("请重新登录");
			window.top.location.href = 'wxlogin.html';
			return false;
		}
		getFeedStandardList();
	});
	function getFeedStandardList(){
		$.ajax({
			type: "GET",
			url: commonIP + 'api/pig/feedstandard/getFeedStandardList',
			contentType: 'application/json',
			data: 'authToken=' + authToken + '&farmId='+farmId,
			dataType: 'json', // GET 请求方式需要添加dataType 设定返回数据的格式为json;
			success: function(data) {
				console.log(data)
				if (data) {
					if (data.errcode == 200) {
						var result = "";
						for(var key in data.dataSource.list){
							var stageId = data.dataSource.list[key].stageId;//生产阶段ID
							var stageName = data.dataSource.list[key].stageName;//配种阶段名称
							var pigTypeName = data.dataSource.list[key].pigTypeName;//猪只类型
							var feedType = data.dataSource.list[key].feedType;//饲料类型
							var feedAmount = data.dataSource.list[key].feedAmount;//料量
							var numberTimes = data.dataSource.list[key].numberTimes;//次数
							var remark = data.dataSource.list[key].remark;//备注
							result = result + '<tr><td rowspan="">'+stageName+'</td><td>'+pigTypeName+'</td><td>'+feedType+'</td><td>'+feedAmount+'KG</td><td>'+numberTimes+'次</td><td>'+remark+'</td></tr>';
						}
						$('#feedStandard').append(result);
					} else if (data.errcode == "10005") {
						window.top.location.href = 'wxlogin.html';
					}
				}
			},
			error: function(err) {
	
			}
		})
	}
</script>
