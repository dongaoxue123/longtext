select[name="data"]{
	min-width: 100px;
	height: 36px;
	line-height: 36px;
	border-radius: 4px;
	border: 1px solid #25B4F5;
	background: url("../style/arrow_blue.png") no-repeat scroll 96% center transparent;
}
select[name="factory"]{
	min-width: 200px;
	height: 20px;
	line-height: 20px;
	font-size: 16px;
	border-radius: 4px;
