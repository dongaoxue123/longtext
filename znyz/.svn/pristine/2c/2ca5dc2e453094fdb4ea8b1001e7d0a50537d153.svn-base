.btn{
	height: 2.2rem;
}
.addlist{
	width: 96%;
	margin: 0 auto;
	padding-top: 0.4rem;
}
.addlist ul li{
	position: relative;
	width: 90%;
	margin: 0 auto;
	height: 1.5rem;
	line-height: 1.5rem;
	font-size: 0.4375rem;
	border-bottom: 1px solid #e0e0e0;
    border-radius: 0;
    background: #FFFFFF;
    text-indent: 0.1rem;
    margin-bottom: 0.2rem;
}
.addlist ul li > span{
	display: inline-block;
	width: 2.5rem;
}
.addlist ul li input{
	width: 60%;
	height: 0.8rem;
	line-height: 0.8rem;
	border: none;
	border-radius: 6px;
	padding-left: 0.2rem;
	-moz-appearance: none;
	vertical-align: middle;
}
input[name="save"]{
	display: block;
	width:90%;
	margin: 0.5rem 5% 0.3rem;
	height: 1.2rem;
	border: 0;
	float: left;
	/*background: #00bc97;*/
	background: #25B4F5;
	font-size: 0.4375rem;
	color: #fff;
	-webkit-appearance: none;
	border-radius: 6px;
}
input[name="cancel"]{
	display: block;
	width:90%;
	margin:0 5% 0.5rem;
	height: 1.2rem;
	border: 0;
	float: left;
	background: #eee;
	font-size: 0.4375rem;
	color: #2A3139;
	-webkit-appearance: none;
	border-radius: 6px;
}
.edit{
	position: absolute;
	right: 1%;
	top: 0.05rem;
}
.edit1{
	position: absolute;
	right: 3%;
	top: 0.31rem;
}
.edit span{
	font-size: 0.6rem;
	color: #00bc97;
}
input{padding-top:0;padding-bottom:0;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box}
input[type="button"], input[type="submit"], input[type="reset"] {
	-webkit-appearance: none;
}

.footer ul li.active{
	color: #00bc97;
}
.footer ul li.active .iconfont{
	color: #00bc97;
}
